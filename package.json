{
  "name": "SPA-boilerplate",
  "version": "0.0.1",
  "description": "SPA biolerplate!",
  "main": "dist/index.html",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crazy-Ivan/SPA-boilerplate.git"
  },
  "bugs": "https://github.com/Crazy-Ivan/SPA-boilerplate/issues",
  "author": {
    "name": "Paweł Bród",
    "email": "pawel.brod@gmail.com",
    "url": "http://github.com/Crazy-Ivan"
  },
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run",
    "build": "gulp build",
    "docs": "npm run sassdocs && npm run jsdocs",
    "start": "gulp",
    "buildjs": "jspm bundle-sfx application/app dist/app.min.js --minify --skip-source-maps",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc src/assets/scss",
    "deploy": "gulp deploy"
  },
  "keywords": [
    "es6",
    "jspm",
    "gulp",
    "babel",
    "systemjs"
  ],
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "src/lib"
  },
  "devDependencies": {
    "del": "^1.2.0",
    "esdoc": "^0.1.2",
    "eslint": "^0.22.1",
    "eslint-config-standard": "^3.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.5.2",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^0.13.2",
    "gulp-gh-pages": "^0.5.2",
    "gulp-imagemin": "^2.2.1",
    "gulp-minify-css": "^1.1.6",
    "gulp-minify-html": "^1.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-sass": "^2.0.1",
    "gulp-scss-lint": "^0.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.5",
    "imagemin-pngquant": "^4.1.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.36",
    "karma-jasmine": "^0.3.5",
    "karma-jspm": "^1.1.5",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-phantomjs-shim": "^1.0.0",
    "phantomjs": "^1.9.17",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.0",
    "sassdoc": "^2.1.15"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib",
      "test": "test"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
      "babel-runtime": "npm:babel-runtime@^5.1.13",
      "core-js": "npm:core-js@^0.9.4"
    }
  }
}
